   "source": [
    "SELECT\n",
    "    b.title AS ProductName,\n",
    "    SUM(o.quantity) AS TotalQuantitySold\n",
    "FROM\n",
    "    books b\n",
    "JOIN\n",
    "    orders o ON b.book_id = o.book_id\n",
    "GROUP BY\n",
    "    b.book_id, b.title\n",
    "ORDER BY\n",
    "    TotalQuantitySold DESC\n",
    "LIMIT 3;\n"
   ]
  }
 ],
